local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hominhtrieu56-spec/TrieuHentaiz/refs/heads/main/TrieuHentaiz')))()

local Window = OrionLib:MakeWindow({Name = "ZingMod", HidePremium = false, SaveConfig = true, ConfigFolder = "dxl_bf"})

local Tab = Window:MakeTab({
	Name = "HixBox",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

